@using MVC_221.Entity
@model List<Customers>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>

        <table border="1">
            <tr>
                <td><b>Kişi</b></td>
                <td><b>Şirket Adı</b></td>
                <td><b>Şehir</b></td>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.ContactName</td>
                    <td>@item.CompanyName</td>
                    <td>@item.City</td>
                </tr>
            }
            </table>

    </div>
</body>
</html>
